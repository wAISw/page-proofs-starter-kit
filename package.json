{
  "name": "page-proofs-starter-kit",
  "version": "0.0.1",
  "engines": {
    "node": ">=6.4"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "clean": "del-cli target",
    "lint": "eslint -c .eslintrc.yml *.js src server",
    "prebuild": "run-s clean lint",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start"
  },
  "devDependencies": {
    "autoprefixer": "~6.4.1",
    "babel-core": "~6.14.0",
    "babel-eslint": "~6.1.2",
    "babel-loader": "~6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "~6.14.0",
    "babel-preset-stage-0": "~6.5.0",
    "cross-env": "~3.0.0",
    "css-loader": "~0.25.0",
    "del-cli": "~0.2.0",
    "eslint": "~3.6.0",
    "express": "~4.14.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "firebase": "~3.4.0",
    "firebase-tools": "3.0.7",
    "html-webpack-plugin": "~2.22.0",
    "image-webpack-loader": "^3.0.0",
    "jasmine-core": "~2.5.2",
    "karma": "~1.3.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-jasmine": "~1.0.2",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.8.0",
    "minx": "r-park/minx.git",
    "morgan": "~1.7.0",
    "node-sass": "~3.10.0",
    "node-static": "^0.7.9",
    "nodemon": "~1.10.2",
    "npm-run-all": "~3.1.0",
    "postcss-loader": "~0.13.0",
    "sass-loader": "~4.0.2",
    "sinon": "~1.17.6",
    "style-loader": "~0.13.1",
    "webpack": "~1.13.2",
    "webpack-dev-server": "~1.16.1",
    "webpack-md5-hash": "~0.0.5",
    "winston": "~2.2.0"
  }
}
